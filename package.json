		"storybook": "7.0.0-beta.62",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"@floating-ui/dom": "^1.2.7",
		"vite": "^4.0.0"
	},
	"type": "module"
